{"components/cart/item-giftwrap":"<div class=\"cart-notes-container\" data-product-name=\"{{name}}\">\n    {{#if gift_wrapping}}\n\n        {{#if name '==' 'CUSTOM BON BOX'}}\n        {{else}}\n            <div class=\"cart-notes-message-container\">\n                <p class=\"cart-notes-message\" data-item-note=\"{{id}}\">{{gift_wrapping.message}}</p>\n                <div>\n                    <button class=\"cart-notes--edit-button\">EDIT PERSONAL NOTE</button>\n                </div>\n            </div>\n            <div class=\"cart-notes-input-container cart-notes--invisible\">\n                <textarea cols=\"30\" rows=\"10\"></textarea>\n                <div class=\"cart-notes-input-container--actions\">\n                    <button class=\"cart-notes-input-container--save-button\" data-item-note=\"{{id}}\">SAVE</button>\n                    <button class=\"cart-notes-input-container--cancel-button\">CANCEL</button>\n                </div>\n            </div>\n        {{/if}}\n    {{else}}\n\n        {{#if name '==' 'CUSTOM BON BOX'}}\n        {{else}}\n            <div class=\"cart-notes--add-link\">\n                <button class=\"cart-notes--add-button cart-notes--invisible\">ADD PERSONAL NOTE</button>\n            </div>\n            <div class=\"cart-notes-input-container cart-notes--invisible\">\n                <textarea cols=\"30\" rows=\"10\"></textarea>\n                <div class=\"cart-notes-input-container--actions\">\n                    <button class=\"cart-notes-input-container--save-button\" data-item-note=\"{{id}}\">SAVE</button>\n                    <button class=\"cart-notes-input-container--cancel-button\">CANCEL</button>\n                </div>\n            </div>\n        {{/if}}\n    {{/if}}\n</div>\n"}
